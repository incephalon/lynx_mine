@{
    ViewBag.Title = "Index";
}
<link href="~/Content/style.css" rel="stylesheet" />

		
		<script type="text/javascript" src="/javascripts/home.js"></script>


	<div class="col-sm-12">
		<div class="col-xs-8 col-sm-8">
			<div class="row show-grid">
				<div class="col-xs-9 col-sm-9">
				  	<input id="urlText" type="email" class="form-control" id="exampleInputEmail1" placeholder="Browse www">
				</div>
				<div class="col-xs-2 col-sm-2">		
				  	<button id="goButton" type="button" class="btn btn-primary">Search</button>
				</div>
			</div>	
			<div class="col-xs-12 col-sm-12"></div>
			<div class="col-xs-12 col-sm-12"></div>
			<div class="col-xs-12 col-sm-12">
				<div class="row show-grid">
					<iframe id="urlIframe" class="embed-responsive-item" src="http://getbootstrap.com/"></iframe>
				</div>
			</div>
		</div>
		<div class="col-xs-4 col-sm-4">	

			<div class="panel-group" id="accordion">
				  <div class="panel panel-default">
				    <div class="panel-heading">
				      <h4 class="panel-title">
				        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
				          Search your tags..
				        </a>
				      </h4>
				    </div>
				    <div id="collapseOne" class="panel-collapse collapse in">
				      <div class="panel-body">
				        <div class="input-group">

							<input id="goText"  type="text" class="form-control input-large" placeholder="Search your tag..">
							<span id="goTag" class="input-group-addon input-large">
								<span class="glyphicon glyphicon-search"></span>
								<span class="glyphicon glyphicon-refresh glyphicon-refresh-animate" style="display:none;"></span>
							</span>
						</div>					
						<div id="toggelDrop" class="btn-group open">					
							<ul id="showList" class="dropdown-menu" role="menu">						
							</ul>
						</div>
				      </div>
				    </div>
				  </div>			  				  
				</div>

			<div id="tagId" type="text" class="form-control" style="display:none"></div>
			<textarea id="notesElement" class="form-control" rows="3" placeholder="My notes"></textarea>
			<input id="tagElement" type="email" class="form-control"  placeholder="My tag">
			<button id="saveButton" type="button" class="btn btn-primary">Save</button>
			<button id="clearButton" type="button" class="btn btn-primary">clear</button>
		</div>
	</div>
